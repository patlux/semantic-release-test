{
  "name": "@patwoz/semantic-release-test",
  "version": "0.0.0-development",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "check-types": "tsc --noEmit",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint",
    "build": "tsc",
    "semantic-release": "semantic-release",
    "prepublish": "npm run build"
  },
  "author": "Patrick Wozniak <email@patwoz.de> (https://patwoz.de)",
  "homepage": "https://github.com/patlux/semantic-release-test.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/patlux/semantic-release-test.git"
  },
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@typescript-eslint/eslint-plugin": "2.12.0",
    "@typescript-eslint/parser": "2.12.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "semantic-release": "15.13.31",
    "typescript": "3.7.3"
  }
}
